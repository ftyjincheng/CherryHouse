<view class="container"><image src="{{img}}" mode="aspectFill"></image><view class="content"><view class="commission-title">代办房产证</view><view class="_br"></view><text class="info">称呼</text><view class="phone"><input class="phone1" placeholder="请输入称呼" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/><view><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><radio value="0">先生</radio><radio value="1">女士</radio></radio-group></view></view><text class="info">手机号</text><input class="phone1" maxlength="11" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><button class="btn" hover-class="none" data-event-opts="{{[['tap',[['publish',['$event']]]]]}}" bindtap="__e">提交</button><button class="primary" hover-class="none" data-event-opts="{{[['tap',[['transfer',['$event']]]]]}}" bindtap="__e">代办过户</button></view>